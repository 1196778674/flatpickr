{
  "name": "flatpickr-docs",
  "version": "1.0.0",
  "description": "A lightweight, powerful javascript datetime picker",
  "scripts": {
    "build": "run-p build:*",
    "build:layout": "pug site/index.pug -o ./",
    "build:script": "babili ./assets/js --out-file ./dist/site.js",
    "build:style": "stylus --compress --include ./style ./style/_style.styl --out dist",
    "dev": "run-p \"build:* -- --watch\" serve livereload",
    "livereload": "livereload \"./dist, index.html\"",
    "serve": "http-server -o --silent"
  },
  "dependencies": {},
  "devDependencies": {
    "babili": "latest",
    "http-server": "^0.9.0",
    "livereload": "^0.6.0",
    "npm-run-all": "^3.1.2",
    "parallelshell": "^2.0.0",
    "pug": "^2.0.0-beta6",
    "stylus": "^0.54.5"
  }
}
